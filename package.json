{
  "name": "@landrik/ui",
  "version": "0.0.1",
  "description": "a collection of globaldata ui library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "patch-package": "patch-package",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --watchAll --verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": " npm run build && npm publish",
    "component:generate": "./src/scripts/generate-component.sh",
    "component:remove": "./src/scripts/remove-component.sh $1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://landrik@bitbucket.org/landrik/landrik-ui.git"
  },
  "author": "landry karege <landrik@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://bitbucket.org/landrik/landrik-ui/issues"
  },
  "homepage": "https://bitbucket.org/landrik/landrik-ui#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-jest": "^8.0.8",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.79",
    "@types/styled-components": "^5.1.34",
    "babel-jest": "^29.7.0",
    "classnames": "^2.5.1",
    "css-loader": "^7.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.14.3",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "storybook-css-modules": "^1.0.8",
    "style-loader": "^4.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/test/styleMock.ts"
    }
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "chrome": 100,
            "safari": 15,
            "firefox": 91
          }
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {}
    }
  },
  "dependencies": {
    "@fontsource/ibm-plex-sans": "^5.2.5",
    "@fontsource/public-sans": "^5.2.5",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addons": "^7.6.17",
    "clsx": "^2.1.1",
    "react-hook-form": "^7.56.1",
    "react-router": "^7.6.0",
    "react-router-dom": "^7.6.0",
    "react-tabs": "^6.1.0"
  }
}
